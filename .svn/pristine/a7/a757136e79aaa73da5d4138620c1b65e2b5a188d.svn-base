    <script type="text/javascript">



        $(function () {
            // 个人门户→固定导航条
            hexun.home.navBar().init();

            // 个人门户→导航条效果
            hexun.home.banner().init();

            // 个人门户→自定义导航条图片
            hexun.home.bannerPic().init();

            // 个人门户→他的主页
            hexun.home.homepage().init({ state: "ta" });

            hexun.home.upGrade = (function(){
                var defaults = {
                    txt: "",
                    html: "",
                    addClass: "",
                    callback: $.noop,
                    isBg: true,
                    time:1000
                }
                function extendDeep(parent,child){
                    var i,
                            toStr = Object.prototype.toString,
                            astr = "[object Array]";
                    child = child || {};
                    for(i in parent){
                        if(parent.hasOwnProperty(i)){
                            if(typeof parent[i] === "object"){
                                child[i] = (toStr.call(parent[i]) === astr) ? [] : {};
                                extendDeep(parent[i],child[i]);
                            } else{
                                child[i] = parent[i];
                            }
                        }
                    }
                    return child;
                }
                var page = document.body,oTc;

                var initialize = function () {
                    setTimeout(doTc,defaults.time)
                };
                function doTc(){
                    oTc = {
                        ele:null,
                        layer:null,
                        drawPanel: function(){
                            var otc,aHTML,ret = [];
                            otc = document.createElement('div');
                            otc.className = "tcBox";
                            ret.push('<span class="close" id="close"></span>');
                            ret.push('<div class="grade1 clearfix">');
                            ret.push('<div class="fl ml20 grade1L"><span>');
                            ret.push(defaults.txt);
                            ret.push('</span><br>');
                            ret.push(' <a href="');
                            ret.push(defaults.linkHref);
                            ret.push('">');
                            ret.push(defaults.linkText);
                            ret.push('</a></div><div class="fl ml10 grade1R">');
                            ret.push('<a id="btn_grade" href="');
                            ret.push(defaults.linkHref1);
                            ret.push('" class="btnUp">');
                            ret.push(defaults.btnTxt);
                            ret.push('</a><Br><span>');
                            ret.push(defaults.txtWar);
                            ret.push('</span></div></div></div>');
                            aHTML = ret.join('');
                            otc.innerHTML = aHTML;

                            document.body.appendChild(otc);
                            this.ele = otc;

                            this.onResize();
                            this.bindEvent();
                        },
                        bindEvent:function(){
                            var me =  this;
                            var oClose = document.getElementById('close'),
                                    oBtn = document.getElementById('btn_grade');
                            oClose.onclick = function(){
                                me.closeAlert();
                            };
                            oBtn.onclick = function(){
                                defaults.callback();
                                me.closeAlert();
                            };

                        },
                        onResize: function () {
                            var ele = this.ele;
                            ele.style.left = (parseInt(document.documentElement.clientWidth || document.body.clientWidth)) / 2 + (document.documentElement.scrollLeft || document.body.scrollLeft) - ele.offsetWidth / 2 + "px";
                            ele.style.top = 40 + 'px';
                        },
                        closeAlert: function () {
                            if (defaults.isBg) {
                                document.body.removeChild(this.layer); //删除节点
                            }
                            document.body.removeChild(this.ele); //删除节点
                            return false;
                        }
                    };
                    oTc.drawPanel();

                }

                return {
                    init:function(options){
                        extendDeep(options,defaults);
                        initialize();
                    }

                };
            })();

            hexun.home.upGrade.init({
                txt:"和讯新版个人门户升级了！新版功能全新上线！",
                linkText:'查看新版个人门户功能和特色',
                linkHref:'#',
                linkHref1:'#',
                btnTxt:'一键升级',
                txtWar:'升级后不可恢复旧版',
                callback:function(){
                    alert("升级程序");
                }
            });

        });

    </script>
